<% include layout.ejs %>
<div class="container">
<div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img class="d-block w-100" height="350px" src="../../images/contact_us1.jpg" alt="First slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" height="350px" src="../../images/contact_us2.jpg" alt="Second slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" height="350px" src="../../images/contact_us3.jpg" alt="Third slide">
    </div>
  </div>
  <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>

<br>
<div class="container">
  <div class="row">
    <div class="col-7">

<div class="jumbotron w-100 p-3">

<form class="form-group needs-validation" novalidate ng-controller="contactCtrl" id="contact" ng-init="getUser()">
  <h2 class="header_color align-items-center">Contact us</h2>
  <p class="text-danger">Got a question? we'd love to hear from you. Send us a message and we'll respond as soon as possible.</p>
  <b> <label class="header_color" id="pas" for="Name">Name</label></b>
  <input class="form-control" type="text" id="Name" value="" required ng-model="contactForm.name" /><br>
  <div class="valid-feedback">
    Looks good!
   </div>
   <div class="invalid-feedback">
     Please provide Name / valid Name!
   </div>
  <b> <label class="header_color"  for="email">Email</label></b>
  <input class="form-control" type="text" id="email" ng-model="contactForm.email" required pattern="^[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,4}$"/><br>
  <div class="valid-feedback">
    Looks good!
   </div>
   <div class="invalid-feedback">
     Please provide Email / valid Email!
   </div>

  <b> <label class="header_color" id="pas" for="text" >Enter your message</label></b><br>
  <textarea class="form-control" name="text" required  ng-model="contactForm.message" ></textarea><br>
  <div class="valid-feedback">
    Looks good!
   </div>
   <div class="invalid-feedback">
     Please provide Message / valid Message!
   </div>

<hr>
  <div class="row">
    <div class="col">
      <div class="form-group">
        <button class="btn btn-primary" > Send </button>
    </div> 
 </div>
  <div class="col">
    <div class="form-group">
      <button class="btn btn-primary"  ng-Click="goToHome()" > Cancel </button>
    </div> 
  </div>
</div>
</form>
</div>
</div>


<div class="col-1">
  <br><br><br><br>
<img src="../../images/location.png" alt=""><br><br><br><br>
<img src="../../images/phone.png" alt=""><br>


</div>
<div class="col-4">
  <br><br><br><br>
<h6 class="header_color">Loanalytic</h6>
<h6 class="header_color">  800 University Drive<h6>
<h6 class="header_color">Maryville, MO 64468 USA</h6><br>

<h6 class="header_color">660.562.1212</h6>
<h6 class="header_color">660.562.1213</h6>
<h6 class="header_color">660.562.1214</h6><br><br>

<h4 class="header_color">Stay connected</h4>
<img src="../../images/facebook.png" alt="">
<img src="../../images/linkedin.png" alt="">
<img src="../../images/twitter.png" alt="">
<img src="../../images/chat.png" alt="">


</div>
</div>
</div>
</div>

<div class="loading" id="spinner">
  <div class="loader"></div>
</div>
<div id="registerModal" class="modal fade">
  <div class="modal-dialog modal-confirm">
     <div class="modal-content">
        <div class="modal-header">
           <div class="icon-box">
              <i class="material-icons">check_circle_outline</i>
           </div>				
           <h4 class="modal-title">Success!</h4>	
        </div>
        <div class="modal-body">
           <p class="text-center">Email has been sent to Support Team</p>
        </div>
        <div class="modal-footer">
           <button class="btn btn-success btn-block" data-dismiss="modal">OK</button>
        </div>
     </div>
  </div>
</div>
<div class="container pull-right" id="chatbox">
  <div class="row chat-window col-xs-5 col-md-3 pull-right" id="chat_window_1">
     <div class="col-xs-12 col-md-12 col-sm-12">
        <div class="panel panel-default">
           <div class="panel-heading top-bar">
              <div class="row">
                 <div class="col-md-7 col-xs-8 col-sm-8 ">
                    <h3 class="panel-title"><span class="fa fa-comment"></span> Chat</h3>
                 </div>
                 <div class="col-md-4 col-xs-4 col-sm-4 " style="text-align: right;">
                    <a class="min_icon" href="#"><span id="minim_chat_window" class="fa fa-minus icon_minim"></span></a>
                     <!--<a href="#"><span class="fa fa-remove icon_close" data-id="chat_window_1"></span></a>-->
                 </div>
              </div>
           </div>
           <div class="panel-body">
           <div id="messagebody" class="msg_container_base">
           
           </div>
           <div id="notifyUser"></div>
           <div class="panel-footer">
                   <div class="input-group">
                       <input type="hidden" id="user" value="" />
                      <input id="btn-input" type="text" onkeyup="notifyTyping();" class="form-control input-sm chat_input" placeholder="Write your message here..." required="required" />
                      <span class="input-group-btn">
                      <button class="btn btn-primary btn-sm" id="btn-chat">Send</button>
                      </span>
                   </div>
                </div>
       </div>
           
        </div>
       
     </div>
  </div>
</div>
      <% include footer.ejs %>
      <script src="../../js/contact.js"></script>
