<% include layout.ejs %>
<div class="container-fluid">
        <div class="row">
    <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
          <ol class="carousel-indicators">
            <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="3"></li>
          </ol>
          <div class="carousel-inner">
            <div class="carousel-item active">
              <img class="d-block w-100" src="../../images/businessloans.jpg" width="100%" height="500px" alt="First slide">
              <!--<div class="carousel-caption d-none d-md-block">
                <h5></h5>
                <p></p>
            </div>-->
            </div>
            <div class="carousel-item">
              <img class="d-block w-100" src="../../images/educationloans.jpg" width="100%" height="500px" alt="Fourth slide">
              
          </div>
            <div class="carousel-item">
              <img class="d-block w-100" src="../../images/moneylending.jpg" width="100%" height="500px" alt="Second slide">
              
            </div>
            <div class="carousel-item">
              <img class="d-block w-100" src="../../images/Personal-Loans.jpg" width="100%" height="500px" alt="Third slide">
              
            </div>
            <div class="carousel-item">
              <img class="d-block w-100" src="../../images/autoloans.png" width="100%" height="500px" alt="Fourth slide">
          
          </div>
          <div class="carousel-item">
            <img class="d-block w-100" src="../../images/money_makin-cover.jpg" width="100%" height="500px" alt="Fourth slide">
            
        </div>
          <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
          </a>
          <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
          </a>
        </div>
      </div>
      </div>
      <div class="row cardlower">
        <div class="card-deck">
         <!-- Card -->
              <div class="card">
                <!-- Card image -->
                <div class="view overlay">
                  <img class="card-img-top" src="../../images/personalloan.jpg" height="250px" alt="Card image cap">
                  <a href="#!">
                    <div class="mask rgba-white-slight"></div>
                  </a>
                </div>
                <!-- Card content -->
                <div class="card-body">
                  <!-- Title -->
                  <h4 class="card-title">Personal</h4>
                  <!-- Text -->
                  <p class="card-text">
                    A personal loan is a type of unsecured loan and helps you meet your current financial needs.</p>
                  <!-- Button -->
                  <a href="#" class="btn btn-primary">Get Quote</a>
                </div>
              </div>
              <!-- Card -->
              <div class="card">
                <!-- Card image -->
                <div class="view overlay">
                  <img class="card-img-top" src="../../images/business.jpeg" height="250px" alt="Card image cap">
                  <a href="#!">
                    <div class="mask rgba-white-slight"></div>
                  </a>
                </div>
                <!-- Card content -->
                <div class="card-body">
                  <!-- Title -->
                  <h4 class="card-title">Business</h4>
                  <!-- Text -->
                  <p class="card-text">A business loan is a loan specifically intended for business purposes. As with all loans, it involves the creation of a debt, which will be repaid with added interest.</p>
                  <!-- Button -->
                  <a href="#" class="btn btn-primary">Get Quote</a>
                </div>
              </div>
              <div class="card">
                <!-- Card image -->
                <div class="view overlay">
                  <img class="card-img-top" src="../../images/auto.jpg"  height="250px" alt="Card image cap">
                  <a href="#!">
                    <div class="mask rgba-white-slight"></div>
                  </a>
                </div>
                <!-- Card content -->
                <div class="card-body">
                  <!-- Title -->
                  <h4 class="card-title">Auto</h4>
                  <!-- Text -->
                  <p class="card-text">An auto loan is a type of secured loan, which means that the car you buy is collateral for the loan</p>
                  <!-- Button -->
                  <a href="#" class="btn btn-primary">Get Quote</a>
                </div>
              </div>
        </div>
      </div>
      <div class="row cardlower">
        <div class="card-deck">
         <!-- Card -->
              <div class="card">
                <!-- Card image -->
                <div class="view overlay">
                  <img class="card-img-top" src="../../images/education.jpg" height="250px" alt="Card image cap">
                  <a href="#!">
                    <div class="mask rgba-white-slight"></div>
                  </a>
                </div>
                <!-- Card content -->
                <div class="card-body">
                  <!-- Title -->
                  <h4 class="card-title">Education</h4>
                  <!-- Text -->
                  <p class="card-text">A student loan is a type of loan designed to help students pay for post-secondary education and the associated fees, such as tuition, books and supplies, and living expenses.</p>
                  <!-- Button -->
                  <a href="#" class="btn btn-primary">Get Quote</a>
                </div>
              </div>
              <!-- Card -->
              <div class="card">
                <!-- Card image -->
                <div class="view overlay">
                  <img class="card-img-top" src="../../images/homeinsurance.jpg" height="250px" alt="Card image cap">
                  <a href="#!">
                    <div class="mask rgba-white-slight"></div>
                  </a>
                </div>
                <!-- Card content -->
                <div class="card-body">
                  <!-- Title -->
                  <h4 class="card-title">Home</h4>
                  <!-- Text -->
                  <p class="card-text"> A house loan or home loan simply means a sum of money borrowed from a financial institution or bank to purchase a house</p>
                  <!-- Button -->
                  <a href="#" class="btn btn-primary">Get Quote</a>
                </div>
              </div>
              <div class="card">
                <!-- Card image -->
                <div class="view overlay">
                  <img class="card-img-top" src="../../images/mortgage.jpg"  height="250px" alt="Card image cap">
                  <a href="#!">
                    <div class="mask rgba-white-slight"></div>
                  </a>
                </div>
                <!-- Card content -->
                <div class="card-body">
                  <!-- Title -->
                  <h4 class="card-title">Mortgage</h4>
                  <!-- Text -->
                  <p class="card-text">A mortgage is a loan in which property or real estate is used as collateral.</p>
                  <!-- Button -->
                  <a href="#" class="btn btn-primary">Get Quote</a>
                </div>
              </div>
        </div>
      </div>
      </div>
      <% include footer.ejs %>
      <style>
        a.btn.btn-primary.waves-effect.waves-light {
          background: #008f95 !important;
          border: none;
      }
      </style>
    </body>
